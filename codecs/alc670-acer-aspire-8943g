!!User specific config file (~/.asoundrc)

pcm.jackplug {
    type plug
    slave { pcm "jack" }
}

pcm.jack {
	type jack
	playback_ports {
		0 alsa_pcm:playback_1
		1 alsa_pcm:playback_1
	}
	capture_ports {
		0 alsa_pcm:capture_1
		1 alsa_pcm:pcapture_1
	}
}


